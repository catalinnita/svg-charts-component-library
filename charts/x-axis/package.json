{
    "name": "@scrambled-data/x-axis",
    "displayName": "xAxis",
    "description": "The *xAxis* is used to show the important points on horizontal axis.",
    "version": "1.0.1",
    "main": "dist/index.js",
    "module": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "utl": "https://github.com/catalinnita/svg-charts-component-library"
    },
    "scripts": {
        "build": "tsc src/index.tsx --declaration --jsx react-jsx --outDir ./dist",
        "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
        "lint": "TIMING=1 eslint src --fix",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "devDependencies": {
        "@scrambled-data/tsconfig": "^1.0.1",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5"
    },
    "dependencies": {
        "@scrambled-data/line-horizontal": "*",
        "@scrambled-data/line-vertical": "*"
    }
}
